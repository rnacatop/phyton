listasw=[]
listaR=[]
listaDF=[]
lista= ["S1","S2","S3","S4","R1","R2","R3","R4","Pc","Ps5","Ps4",]

for i in lista:
    if 'S' in i:
        print (i,end=" ")
        listasw.append(i)
    elif 'R' in i:
        listaR.append(i)
    else:
        listaDF.append(i)
print('Switvhes: ',listasw,'/nRouters: ',listaR,'/nDispositivos Finales: ',listaDF)
     
