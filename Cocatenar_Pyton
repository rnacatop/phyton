str1= "Cisco"
str2="Networking"
str3="Academy"
space=" "
print(str1+space+str2+space+str3)
print('\n')
print(str1+str2+str3)
print('\n')
print(str1,str2,str3)
x=5
print(type(x),'\n')
print('El valor de x es: '+str(x),'\n')
print(type(x),'\n')
x=str(x)
print(type(x),'\n')
x=bool(x)
print(type(x),'\n')
x=float(x)
print(type(x),'\n')
